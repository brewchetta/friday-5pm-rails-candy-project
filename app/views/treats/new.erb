<h1>Forge Your Treat!</h1>

<%= form_with model: @treat do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :rating %>
  <%= f.number_field :rating %>

  <%= f.label :ingredients %>
  <%= f.text_area :ingredients %>

  <%= f.label :picture_url %>
  <%= f.text_field :picture_url %>

  <%= f.submit %>

<% end %>
