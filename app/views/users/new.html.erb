<h1>Sign Up For Delicious Treats</h1>

<%= form_with model: @user do |form| %>

  <%= form.label :name %>
  <%= form.text_field :name %>

  <%= form.label :email %>
  <%= form.text_field :email %>

  <%= form.label :password %>
  <%= form.text_field :password %>

  <%= form.label :picture_url %>
  <%= form.text_field :picture_url %>

  <%= form.submit %>

<% end %>
